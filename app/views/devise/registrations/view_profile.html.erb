<div class="row">
	<div class="col-sm-4"></div>
	<div class="col-sm-4">
		<table class="table table-responsive table-bordered table-hover table-condensed">
			<tr class="active">
				<th> Student id :</th>
				<td> <%= "  "+@user.student_id%></td>
			</tr>
			<tr class="active">
				<th> Name :</th>
				<td> <%= "  "+@user.first_name+"  "+@user.last_name%></td>
			</tr>
			<tr class="active">
				<th> Date of Birth :</th>
				<td> <%= "  "+@user.date_of_birth.to_s%></td>
			</tr>

			<tr class="active">
				<th> Email :</th>
				<td> <%= "  "+@user.email%></td>
			</tr>

			<tr class="active">
				<th> Contact:</th>
				<td> <%= "  "+@user.phone_no%></td>
			</tr>
			<tr class="active">
				<th> Degree :</th>
				<td> <%= "  "+@user.degree%></td>
			</tr>
			<tr class="active">
				<th> Passing Year :</th>
				<td> <%= "  "+@user.passing_year.to_s%></td>
			</tr>
			<tr>
				<td colspan="2">
					<center><button class="btn btn-success" onclick="goBack()">Back</button></center>
				</td>
				<td>
					<a href="" class="btn">View Answersheet</a>
				</td>
			</tr>
		</table>
	</div>
</div>

<script type="text/javascript">
	 goBack = function(){
		
		window.history.go(-1);
	}
</script>
