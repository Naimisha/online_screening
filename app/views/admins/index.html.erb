<center>

<div class="row">
	<div class="col-sm-4"></div>
	<div class="col-sm-4">
	<% if flash[:error] %>
		<div class="alert alert-warning">
      		<a href="#" class="close" data-dismiss="alert">&times;</a>
	  		<p class="text text-danger">
						<%= flash[:error] %>
			</p>
		</div>
	<% end %>
	<%= form_for( :admins, url:  { action: "add_users" }, :multipart => true) do |f|  %>
		<table class="table table-bordered">
			<tr style="background-color: green">
				<th class="white-link"><center><i class="glyphicon glyphicon-upload"></i>&nbsp;<%= f.label "Upload Excel Sheet" %></center></th>
			</tr>
			<tr>
				<td>
					<%= f.text_field :college_name, :class => "form-control",:placeholder => "College Name" %>
				</td>
			</tr>
			<tr>
				<td>
					<center>
						<%= f.file_field :userdata, :class => "form-control" %><br>
						<%= f.submit "Upload and Create Users", :class => "btn btn-success" %>
					</center>
				</td>
			</tr>
		</table>
	<% end %>
	</div>
</div>
<div class="row">
	<div class="col-sm-2"></div>
	<div class="col-sm-8">
		<table class="table table-bordered">
			<tr style="background-color: green">
				<th class="white-link"><center><%= label_tag "Excel Sheet Format With Sample Data" %></center></th>
			</tr>
			<tr>
				<td>
					<center><%= image_tag "sample_xls.png" %></center>
				</td>
			</tr>
		</table>
	</div>
	<div class="col-sm-2"></div>
</div>
</center>